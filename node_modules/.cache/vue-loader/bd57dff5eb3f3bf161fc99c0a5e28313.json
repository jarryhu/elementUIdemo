dex, row) {\r\n      this.editdialogVisible = true\r\n      this.eid = row.id\r\n      console.log(index, row)\r\n    },\r\n    handleDelete (index, row) {\r\n      console.log(index, row)\r\n    },\r\n    handlerSearch (searchName) {\r\n      this.$http.post('getProductByName2', { productName: this.searchName },\r\n        { emulateJSON: true }).then(d => {\r\n        if (d.body.code === 0) {\r\n          this.tableData = d.body.data\r\n        }\r\n      })\r\n    }\r\n  },\r\n  created () {\r\n    this.$http.get('getProductList').then(d => {\r\n      console.log(d)\r\n      this.tableData = d.body.data\r\n    })\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]}]}