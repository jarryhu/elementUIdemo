 name: 'Edit',\r\n  props: ['eid'],\r\n  data () {\r\n    return {\r\n      ruleForm: {\r\n        id: '',\r\n        total: '',\r\n        product_price: '',\r\n        productName: ''\r\n      },\r\n      rules: {\r\n        productName: [\r\n          {\r\n            required: true,\r\n            message: '请输入商品名称',\r\n            trigger: 'blur'\r\n          },\r\n          {\r\n            min: 3,\r\n            max: 5,\r\n            message: '长度在 3 到 5 个字符',\r\n            trigger: 'blur'\r\n          }\r\n        ],\r\n        product_price: [\r\n          {\r\n            required: true,\r\n            message: '请输入商品价格',\r\n            trigger: 'blur'\r\n          },\r\n          {\r\n            type: 'number',\r\n            min: 1,\r\n            message: '商品价格不能小于0',\r\n            trigger: 'blur'\r\n          }\r\n        ],\r\n        total: [\r\n          {\r\n            required: true,\r\n            message: '请输入商品数量',\r\n            trigger: 'blur'\r\n          },\r\n          {\r\n            type: 'number',\r\n            min: 1,\r\n            message: '商品数量不能小于0',\r\n            trigger: 'blur'\r\n          }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    submitForm (formName) {\r\n      console.log(this.eid)\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          this.$http.post('http://localhost:8001/updateProduct',\r\n            this.ruleForm, { emulateJSON: true }).then(\r\n            function (d) {\r\n              if (d.body.code === 0) {\r\n                console.log(d)\r\n                parent.location.reload()\r\n              }\r\n            }\r\n          )\r\n        } else {\r\n          console.log('error submit!!')\r\n          return false\r\n        }\r\n      })\r\n    },\r\n    init (id) {\r\n      console.log('eid>>>>' + this.eid)\r\n      this.$http.get('selectProductById/' + id).then(\r\n        function (d) {\r\n          if (d.body.code === 0) {\r\n            var product = d.body.result\r\n            this.ruleForm.id = product.id\r\n            this.ruleForm.total = product.total\r\n            this.ruleForm.product_price = product.product_price\r\n            this.ruleForm.productName = product.productName\r\n          }\r\n        }\r\n      )\r\n    },\r\n    resetForm (formName) {\r\n      this.$refs[formName].resetFields()\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views"}]}