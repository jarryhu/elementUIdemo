AA,GAAA,CAAA,gBAAA,EAAA,IAAA,CAAA,UAAA,CAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,MAAA,MAAA,CAAA,SAAA,GAAA,CAAA,CAAA,IAAA,CAAA,IAAA;AACA,KAHA;AAIA;AAvCA,CAAA","sourcesContent":["<template>\r\n  <div style=\"border: #42b983 solid 1px;padding: 0px\">\r\n    <el-container>\r\n\r\n      <el-main>\r\n        <el-row>\r\n          <div style=\"border: #42b983 solid 1px; text-align: left;padding: 10px;margin-left: 20px\">\r\n            <el-input style=\"width: 200px\"\r\n                      placeholder=\"请输入内容\"\r\n                      v-model=\"searchName\"\r\n                      clearable>\r\n            </el-input>\r\n            <el-button icon=\"el-icon-search\" circle style=\"margin-left: 20px\"\r\n                       @click=\"handlerSearch(searchName)\"></el-button>\r\n            <el-button type=\"primary\" plain style=\"margin-left: 20px\" @click=\"addialogVisible = true\">添加</el-button>\r\n            <el-button type=\"danger\" plain style=\"margin-left: 20px\">批量删除</el-button>\r\n\r\n          </div>\r\n        </el-row>\r\n        <el-table :data=\"tableData\" height=\"250\">\r\n\r\n          <el-table-column\r\n            type=\"selection\"\r\n            width=\"55\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"id\" label=\"商品id\" sortable width=\"140\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"productName\" label=\"商品名\" width=\"120\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"product_price\" sortable label=\"商品价格\">\r\n          </el-table-column>\r\n\r\n          <el-table-column prop=\"total\" label=\"商品数量\">\r\n          </el-table-column>\r\n          <el-table-column label=\"操作\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button\r\n                size=\"mini\"\r\n                @click=\"handleEdit(scope.$index, scope.row)\">编辑\r\n              </el-button>\r\n              <el-button\r\n                size=\"mini\"\r\n                type=\"danger\"\r\n                @click=\"handleDelete(scope.$index, scope.row)\">删除\r\n              </el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </el-main>\r\n    </el-container>\r\n\r\n    <el-dialog title=\"添加商品\" :visible.sync=\"addialogVisible\" width=\"50%\">\r\n      <add-product></add-product>\r\n    </el-dialog>\r\n    <el-dialog title=\"修改商品\" :visible.sync=\"editdialogVisible\" width=\"50%\" destroy-on-close>\r\n      <edit-product :eid=\"eid\"></edit-product>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport AddProduct from './AddProduct'\r\nimport EditProduct from './EditProduct'\r\n\r\nexport default {\r\n  name: 'mainPage',\r\n  components: {\r\n    EditProduct,\r\n    AddProduct\r\n  },\r\n  data () {\r\n    const item = {}\r\n    return {\r\n      tableData: Array(20).fill(item),\r\n      searchName: '',\r\n      addialogVisible: false,\r\n      editdialogVisible: false,\r\n      eid: 0\r\n    }\r\n  },\r\n  methods: {\r\n    handleEdit (index, row) {\r\n      this.editdialogVisible = true\r\n      this.eid = row.id\r\n      console.log(index, row)\r\n    },\r\n    handleDelete (index, row) {\r\n      console.log(index, row)\r\n    },\r\n    handlerSearch (searchName) {\r\n      this.$http.post('getProductByName2', { productName: this.searchName },\r\n        { emulateJSON: true }).then(d => {\r\n        if (d.body.code === 0) {\r\n          this.tableData = d.body.data\r\n        }\r\n      })\r\n    }\r\n  },\r\n  created () {\r\n    this.$http.get('getProductList').then(d => {\r\n      console.log(d)\r\n      this.tableData = d.body.data\r\n    })\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views"}]}